hero.moveDown()

# Mama always said to eat random mushrooms you find in dungeons.
hero.moveRight()
hero.moveDown()
hero.moveUp()
hero.moveLeft()
# Find your way to the Dungeon Keeper.
hero.moveDown(2)
hero.moveRight(4)
hero.moveUp()
hero.moveLeft()
hero.moveUp(2)
hero.moveRight()
hero.moveDown()
hero.moveUp()
hero.moveLeft(2)
while True:
    enemy = hero.findNearestEnemy()
    if enemy:
        hero.attack(enemy)
    
